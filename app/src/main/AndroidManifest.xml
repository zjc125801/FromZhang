<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zjc.demoapp">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

       <activity android:name=".HelloWorld">
           <intent-filter>
               <action android:name="com.helloworld"></action>
               <category android:name="android.intent.category.DEFAULT"></category>
           </intent-filter>
       </activity>

        <activity android:name=".NormalActivity">
        </activity>
        <activity android:name=".DialogActivity" android:theme="@android:style/Theme.Dialog" >
        </activity>

        <activity android:name=".NormalAndDialog">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".TestActivity">
        </activity>
        <activity android:name=".UiAvtivity">
        </activity>
        <activity android:name=".ListViewActivity">
        </activity>
        <activity android:name=".antivity_token">
        </activity>
        <activity android:name=".Fragment_main">
        </activity>
        <activity android:name=".NewsMain">
        </activity>
    </application>
</manifest>